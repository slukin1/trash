<LinearLayout
	width="match_parent"
	height="wrap_content"
	background="@color/KBaseColorContentBackground"
	orientation="vertical"
	topLeftRadius="8"
	topRightRadius="8">

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		orientation="horizontal">

		<TextView
			width="wrap_content"
			height="wrap_content"
			marginLeft="16"
			marginTop="14"
			textFontWeight="500"
			text="{{@i18n/n_double_coin_confirm_order_msg}}"
			textColor="@color/kColorPrimaryText"
			textSize="16" />

		<View
			width="0"
			height="0"
			weight="1" />

		<ImageView
			width="wrap_content"
			height="wrap_content"
			marginTop="10"
			marginRight="16"
			onClick="@event.productsub.closeSubDialog()"
			src="@drawable/edge_engine_copy_trading_dialog_close" />

	</LinearLayout>

	<View
		width="match_parent"
		height="0.5"
		marginTop="7.5"
		marginBottom="10"
		background="@color/KBaseColorPrimarySeparator" />


	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_double_coin_product_name}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			textFontWeight="400"
			gravity="right"
			text="{{@data.productsub.productName}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_asset_subscribe_number}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			gravity="right"
			textFontWeight="400"
			text="{{@data.productsub.quantity}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />

		<TextView
			width="wrap_content"
			height="wrap_content"
			gravity="right"
			textFontWeight="400"
			marginLeft="4"
			text="{{@data.productsub.subUnit}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_double_coin_earn_target_price}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			textFontWeight="400"
			gravity="right"
			text="{{@data.productsub.hookPrice}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		visibility="{{@data.productsub.withDrawVis}}"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_double_coin_with_draw_price}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			textFontWeight="400"
			gravity="right"
			text="{{@data.productsub.withDrawPrice}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_home_index_earn_time_limit}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			textFontWeight="400"
			gravity="right"
			text="{{@data.productsub.expireDate}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		marginBottom="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_double_coin_yield}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			textFontWeight="400"
			gravity="right"
			text="{{@data.productsub.yield}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<LinearLayout
		width="match_parent"
		height="wrap_content"
		marginTop="6"
		orientation="horizontal"
		paddingLeft="16"
		paddingRight="16">

		<TextView
			width="wrap_content"
			height="wrap_content"
			text="{{@i18n/n_double_coin_estimated_profit}}"
			textFontWeight="400"
			textColor="@color/KBaseColorSecondaryText"
			textSize="14" />

		<TextView
			width="0"
			height="wrap_content"
			weight="1"
			gravity="right"
			textFontWeight="400"
			text="{{@data.productsub.estimatedProfit}}"
			textColor="@color/kColorPrimaryText"
			textSize="14" />


	</LinearLayout>

	<TextView
		width="wrap_content"
		height="wrap_content"
		layout_gravity="right"
		marginTop="2"
		marginRight="16"
		text="{{@i18n/n_double_coin_sub_come_money_new}}"
		textFontWeight="400"
		textColor="@color/eBaseColorThreeLevelText"
		textSize="11" />

	<LinearLayout
		marginLeft="8"
		marginRight="16"
		marginTop="16"
		height="wrap_content"
		orientation="horizontal"
		gravity="left"
		width="match_parent">
		<ImageView
			width="26"
			height="30"
			paddingLeft="8"
			paddingRight="4"
			paddingTop="16"
			src="{{@data.productsub.huobiAgreeImage}}"
			onClick="@event.productsub.huobiSelected()" />
		<TextView
			paddingTop="16"
			width="0"
			weight="1"
			height="wrap_content"
			gravity="left"
			richText="{{@data.productsub.agreeText}}"
			textColor="@color/kColorPrimaryText"
			textSize="12"
			textFontWeight="400"
			maxLines="0"
			onClick="@event.productsub.huobilink()" />
	</LinearLayout>

	<TextView
		width="match_parent"
		height="44"
		marginLeft="16"
		marginTop="16"
		marginRight="16"
		textFontWeight="500"
		marginBottom="16"
		background="{{@data.productsub.doBackColor}}"
		gravity="center"
		text="{{@i18n/n_confirm}}"
		onClick="@event.productsub.doOrder()"
		cornerRadius="4"
		textColor="{{@data.productsub.doColor}}"
		textSize="14" />

	<View
		width="match_parent"
		height="{{@data.commonData.bottomSafeAreaHeight}}" />
</LinearLayout>
