<LinearLayout
	width="match_parent"
	height="match_parent"
	gravity="centerVertical"
	orientation="vertical">
	<View width="match_parent" height="98"/>

	<LinearLayout
		width="match_parent"
		height="0"
		weight="1"
		gravity="centerVertical"
		orientation="vertical"
		topRightRadius="8"
		topLeftRadius="8"
		background="@color/KBaseColorContentBackground">

		<!-- 顶部标题+关闭按钮 -->
		<LinearLayout
			paddingLeft="16"
			paddingRight="0"
			width="match_parent"
			height="44"
			gravity="center"
			orientation="horizontal">
			<TextView
				height="match_parent"
				width="wrap_content"
				gravity="center|left"
				text="{{@i18n/n_bot_edit_volume}}"
				textSize="16"
				textColor="@color/kColorPrimaryText"
				textFontWeight="500" />
			<View weight="1" />
			<LinearLayout
				width="100"
				height="44"
				paddingRight="16"
				gravity="center|right"
				orientation="horizontal"
				onClick="@event.botCopyPop.copyPopCancelClick()">
				<ImageView
					width="28"
					height="28"
					src="@drawable/edge_engine_copy_trading_dialog_close" />
			</LinearLayout>
		</LinearLayout>

		<!-- 分割线 -->
		<View
			background="@color/kColorPrimarySeparator"
			width="match_parent"
			height="0.5" />

		<LinearLayout
			width="match_parent"
			paddingLeft="16"
			paddingRight="16"
			paddingTop="10"
			paddingBottom="10"
			height="wrap_content"
			gravity="left|top"
			orientation="horizontal"
			background="#14FE8731"
			visibility="{{@data.botCopyPop.warningVisibility}}">
			<ImageView width="12" height="16" gravity="center" scaleType="fitCenter" src="@drawable/edge_engine_copy_trading_tip"/>
			<TextView 
				width="0" 
				height="wrap_content" 
				marginLeft="4"
				gravity="left|top"
				textSize="12"
				textFontWeight="400"
				text="{{@data.botCopyPop.copyParams.remindMsg}}"
				textColor="@color/kColorPrimaryText"
				maxLines="0"
				weight="1"/>
		</LinearLayout>

		<Scroller
			height="0"
			orientation="vertical"
			width="match_parent"
			weight="1">

			<LinearLayout
				width="match_parent"
				height="wrap_content"
				orientation="vertical" >

				<LinearLayout
					width="match_parent"
					height="wrap_content"
					marginLeft="16"
					marginTop="16"
					marginRight="16"
					paddingTop="8"
					paddingBottom="4"
					paddingLeft="16"
					paddingRight="16"
					cornerRadius="4"
					orientation="vertical"
					background="@color/kColorTipsBackground">
					<LinearLayout
						width="match_parent"
						height="30"
						gravity="left|center"
						orientation="horizontal">
						<TextView 
							width="wrap_content" 
							height="22" 
							gravity="center"
							textSize="16"
							textFontWeight="500"
							text="{{@data.botCopyPop.copyParams.symbolDisplay}}"
							textColor="@color/kColorPrimaryText"/>
						<TextView 
							width="wrap_content" 
							height="16" 
							gravity="center"
							textSize="10"
							textFontWeight="500"
							paddingLeft="4"
							paddingRight="4"
							marginLeft="4"
							cornerRadius="2"
							borderWidth="0.5"
							borderColor="@color/kColorPrimarySeparator"
							text="{{@data.botCopyPop.copyParams.gridModeDisplay}}"
							textColor="@color/kColorSecondaryText" />
						<TextView 
							width="wrap_content" 
							height="16" 
							gravity="center"
							textSize="10"
							textFontWeight="500"
							paddingLeft="4"
							paddingRight="4"
							marginLeft="4"
							cornerRadius="2"
							borderWidth="0.5"
							borderColor="{{@data.botCopyPop.copyParams.trendTypeBorderColor}}"
							text="{{@data.botCopyPop.copyParams.trendTypeDisplay}}"
							textColor="{{@data.botCopyPop.copyParams.trendTypeColor}}" />
					</LinearLayout>
					<For
						data="@data.botCopyPop.copyParams.paramsList"
						itemKey="element"
						typeKey="cellType">
						<Cell
							width="match_parent"
							height="wrap_content"
							type="1">
							<LinearLayout
								width="match_parent"
								height="wrap_content"
								marginTop="4"
								marginBottom="4"
								gravity="top"
								orientation="horizontal">
								<TextView
									gravity="left"
									height="wrap_content"
									width="0"
									weight="1"
									text="{{@element.title}}"
									textColor="@color/kColorThreeLevelText"
									textFontWeight="400"
									textSize="12"
									maxLines="0" />
								<TextView
									gravity="right"
									height="wrap_content"
									width="140"
									text="{{@element.content}}"
									textColor="@color/kColorPrimaryText"
									textFontWeight="500"
									textSize="12"
									maxLines="0" />
							</LinearLayout>
						</Cell>
						<Cell
							width="match_parent"
							height="8.5"
							type="2">
							<LinearLayout
								width="match_parent"
								marginTop="4"
								height="0.5"
								gravity="center"
								orientation="horizontal"
								background="@color/kColorPrimarySeparator">
							</LinearLayout>
						</Cell>
						<Cell
							width="match_parent"
							height="wrap_content"
							type="3">
							<LinearLayout
								width="match_parent"
								height="wrap_content"
								gravity="center"
								marginTop="4"
								marginBottom="4"
								orientation="horizontal">
								<TextView
									gravity="left|center"
									height="wrap_content"
									width="0"
									text="{{@element.title}}"
									textColor="@color/kColorThreeLevelText"
									textFontWeight="400"
									textSize="12"
									maxLines="0"
									weight="1" />
								<LinearLayout
									width="wrap_content"
									height="wrap_content"
									gravity="center"
									orientation="horizontal">
									<TextView
										gravity="right|center"
										height="wrap_content"
										width="wrap_content"
										text="{{@element.totalProfits}}"
										textColor="{{@element.textColor}}"
										textFontWeight="500"
										textSize="12"/>
									<TextView
										gravity="right|center"
										height="wrap_content"
										width="wrap_content"
										text="{{@element.totalProfitRate}}"
										textColor="{{@element.textColor}}"
										background="{{@element.backgroundColor}}"
										visibility="{{@element.totalProfitRateVisibility}}"
										textFontWeight="500"
										textSize="10"
										cornerRadius="2"
										marginLeft="4"
										paddingLeft="4"
										paddingRight="4"/>
								</LinearLayout>
							</LinearLayout>
						</Cell>
						<Cell
							width="match_parent"
							height="wrap_content"
							type="4">
							<LinearLayout
								width="match_parent"
								height="wrap_content"
								gravity="center"
								orientation="horizontal"
								marginTop="4"
								marginBottom="4"
								onClick="@event.botCopyPop.diyParamClick()">
								<TextView
									gravity="left|center"
									height="wrap_content"
									width="0"
									text="{{@element.title}}"
									textColor="@color/kColorPrimaryText"
									textFontWeight="400"
									maxLines="0"
									textSize="12" 
									marginRight="12"
									weight="1"/>
								<ImageView width="14" height="14" src="@drawable/edge_engine_next_step"/>
							</LinearLayout>
						</Cell>
						<Cell
							width="match_parent"
							height="wrap_content"
							type="5">
							<LinearLayout
								width="match_parent"
								height="wrap_content"
								marginTop="4"
								marginBottom="4"
								gravity="top"
								orientation="horizontal">
								<TextView
									gravity="left"
									height="wrap_content"
									width="0"
									weight="1"
									text="{{@element.title}}"
									textColor="@color/kColorThreeLevelText"
									textFontWeight="400"
									textSize="12"
									maxLines="0" />
								<TextView
									gravity="right"
									height="wrap_content"
									width="140"
									text="{{@element.content}}"
									textColor="@color/kColorPrimaryText"
									textFontWeight="500"
									textSize="12"
									maxLines="0"
									onClick="@event.botCopyPop.copyClick()" />
								<ImageView marginLeft="4" width="14" height="14" src="@drawable/edge_engine_orderid_copy" onClick="@event.botCopyPop.copyClick()"/>
							</LinearLayout>
						</Cell>
					</For>

				</LinearLayout>
				<!-- 3.投资额(保证金) -->
				<LinearLayout
					width="match_parent"
					height="wrap_content"
					gravity="center"
					orientation="vertical"
					paddingLeft="16"
					paddingRight="16"
					paddingTop="24">
					<!-- 标题 -->
					<LinearLayout
						width="match_parent"
						height="20"
						gravity="center"
						orientation="horizontal">
						<BottomLineTextView 
							height="match_parent" 
							width="match_parent"
							gravity="left"
							text="{{@i18n/n_contract_grid_create_title_volume}}"
							textColor="@color/kColorPrimaryText" 
							textFontWeight="500" 
							textSize="14" 
							onClick="@event.botCopyPop.remindClick('volume')"/>
					</LinearLayout>
					<!-- 数量输入 -->
					<LinearLayout
						width="match_parent"
						height="44"
						background="@color/KBaseColorInputBackground"
						cornerRadius="4"
						borderColor="#E94359"
						borderWidth="{{@data.botCopyPop.volume.borderWidth}}"
						gravity="center"
						marginTop="8"
						orientation="horizontal">	
						<EditText
                            textColorHint="@color/eButtonUnenabledBgColor"
							width="1" 
							height="match_parent" 
							marginLeft="12"
							hint="{{@data.botCopyPop.volume.volumeMinHint}}"
							bindText="@data.botCopyPop.volume.volume"
							textFontWeight="400" 
							textSize="14"
							textColor="@color/kColorPrimaryText"
							inputType="numberDecimal"
							textFilter="@dataParser.trade_parser(18,4,@eventParams)"
							onTextChange="@event.botCopyPop.onTextChange('volume',@eventParams)"
							onFocusChange="@event.botCopyPop.onFocusChange('volume',@eventParams)"
							weight="1"/>	
						<LinearLayout 
							width="wrap_content" 
							height="match_parent" 
							gravity="center"
							marginRight="12"
							onClick="@event.botCopyPop.volumeLeverClick()">

							<TextView 
								width="wrap_content" 
								height="match_parent" 
								gravity="center"
								text="{{@data.botCopyPop.volume.volumeLeverDisplay}}"
								textColor="@color/kColorPrimaryText" 
								textFontWeight="400" 
								textSize="14" />
							<ImageView height="12" width="12" src="@drawable/edge_engine_trade_bot_arrow_down" marginLeft="4" />
						</LinearLayout>

					</LinearLayout>
					<TextView 
						width="match_parent" 
						height="wrap_content" 
						gravity="left"
						text="{{@data.botCopyPop.volume.errorMsg}}"
						textColor="#E94359" 
						textFontWeight="400" 
						textSize="12"
						marginTop="8"
						maxLines="0"
						visibility="{{@data.botCopyPop.volume.errorVisibility}}"
						/>
					<TextView 
						width="match_parent" 
						height="wrap_content" 
						gravity="left"
						text="{{@data.botCopyPop.volume.totalVolume}}"
						textColor="@color/kColorSecondaryText" 
						textFontWeight="400" 
						textSize="12"
						marginTop="8"
						maxLines="0"
						visibility="{{@data.botCopyPop.volume.totalVolumeVisibility}}"
						/>
					<!-- 可用 -->
					<LinearLayout
						width="match_parent"
						height="16"
						gravity="center"
						marginTop="8"
						orientation="horizontal"
						onClick="@event.botCopyPop.availableClick()">
						<TextView 
							height="match_parent" 
							width="wrap_content"
							gravity="left|center"
							text="{{@i18n/n_asset_coindetail_available}}"
							textColor="@color/kColorSecondaryText" 
							textFontWeight="400" 
							textSize="12" />
						<View height="match_parent" width="0" weight="1"/>
						<LinearLayout
							width="wrap_content"
							height="match_parent"
							gravity="center"
							orientation="horizontal"
							onClick="@event.botCopyPop.availableClick()">
							<TextView 
								height="match_parent" 
								width="wrap_content"
								gravity="left|center"
								text="{{@data.botCopyPop.volume.availableDisplay}}"
								textColor="@color/kColorPrimaryText" 
								textFontWeight="400" 
								textSize="12" />
							<ImageView height="16" width="16" src="@drawable/edge_engine_trade_bot_transfer" marginLeft="4"/>
						</LinearLayout>

					</LinearLayout>

					<SeekBar
						bubbleShow="ture"
						bubbleTextColor="#FFFFFF"
						bubbleTextSize="11"
						height="14"
						marginTop="8"
						sectionCount="4"
						width="match_parent"
						progress="{{@data.botCopyPop.volume.volumePercent}}"
						onValueChange="@event.botCopyPop.volumeChange(@eventParams)" />

					<!-- 体验金券 -->
					<LinearLayout
						width="match_parent"
						height="16"
						gravity="center"
						marginTop="8"
						orientation="horizontal">
						<BottomLineTextView 
							height="20" 
							width="wrap_content"
							gravity="left"
							text="{{@i18n/n_bot_detail_coupon_title}}"
							textColor="@color/kColorSecondaryText" 
							textFontWeight="400" 
							textSize="12"
							onClick="@event.botCopyPop.remindClick('coupon')"/>
						<View height="match_parent" width="0" weight="1"/>
						<LinearLayout
							width="wrap_content"
							height="match_parent"
							gravity="center"
							orientation="horizontal"
							onClick="@event.botCopyPop.couponClick()">
							<TextView 
								height="match_parent" 
								width="wrap_content"
								gravity="left|center"
								text="{{@data.couponPop.couponTipStr}}"
								textColor="@color/kColorPrimaryText" 
								textFontWeight="{{@data.couponPop.couponTipTextWeight}}" 
								textSize="12" />
							<ImageView height="16" width="16" src="@drawable/edge_engine_trade_bot_coupon" marginLeft="4"/>
						</LinearLayout>

					</LinearLayout>

					<!-- 多头预估强平价 -->
					<LinearLayout
						width="match_parent"
						height="16"
						gravity="center"
						marginTop="8"
						orientation="horizontal"
						visibility="{{@data.botCopyPop.volume.liquidationBuyVisibility}}">
						<BottomLineTextView 
							height="match_parent" 
							width="wrap_content"
							gravity="left"
							text="{{@data.botCopyPop.volume.liquidationPriceBuyTitle}}"
							textColor="@color/kColorSecondaryText" 
							textFontWeight="400" 
							textSize="12"
							onClick="@event.botCopyPop.remindClick('liquidation_buy')" />
						<View height="match_parent" width="0" weight="1"/>
						<TextView 
							height="match_parent" 
							width="wrap_content"
							gravity="left"
							text="{{@data.botCopyPop.volume.liquidationPriceBuyDisplay}}"
							textColor="@color/kColorPrimaryText" 
							textFontWeight="400" 
							textSize="12" />
					</LinearLayout>
					<!-- 空头预估强平价 -->
					<LinearLayout
						width="match_parent"
						height="16"
						gravity="center"
						marginTop="8"
						orientation="horizontal"
						visibility="{{@data.botCopyPop.volume.liquidationSellVisibility}}">
						<BottomLineTextView 
							height="match_parent" 
							width="wrap_content"
							gravity="left"
							text="{{@data.botCopyPop.volume.liquidationPriceSellTitle}}"
							textColor="@color/kColorSecondaryText" 
							textFontWeight="400" 
							textSize="12"
							onClick="@event.botCopyPop.remindClick('liquidation_sell')" />
						<View height="match_parent" width="0" weight="1"/>
						<TextView 
							height="match_parent" 
							width="wrap_content"
							gravity="left"
							text="{{@data.botCopyPop.volume.liquidationPriceSellDisplay}}"
							textColor="@color/kColorPrimaryText" 
							textFontWeight="400" 
							textSize="12" />
					</LinearLayout>
					<TextView 
						width="match_parent" 
						height="wrap_content" 
						gravity="left"
						text="{{@data.botCopyPop.volume.liquidationPriceErrorMsg}}"
						textColor="#E94359" 
						textFontWeight="400" 
						textSize="14"
						marginTop="8"
						maxLines="0"
						visibility="{{@data.botCopyPop.volume.liquidationPriceErrorVisibility}}"
						/>
				</LinearLayout>
				
				<!-- 高级设置 -->
				<LinearLayout
					width="match_parent"
					height="wrap_content"
					gravity="center"
					orientation="vertical"
					paddingLeft="16"
					paddingRight="16"
					paddingTop="24">
					<!-- 标题 -->
					<LinearLayout
						width="match_parent"
						height="20"
						gravity="center"
						orientation="horizontal"
						onClick="@event.botCopyPop.settingClick()">
						<TextView
							height="match_parent"
							width="wrap_content"
							gravity="left"
							text="{{@i18n/n_contract_grid_create_title_set}}"
							textColor="@color/kColorPrimaryText"
							textFontWeight="500"
							textSize="14" />
						<View height="match_parent" width="0" weight="1"/>
						<ImageView height="12" width="12" src="{{@data.botCopyPop.setting.iconSrc}}" />
					</LinearLayout>
					<LinearLayout
						width="match_parent"
						height="wrap_content"
						gravity="center"
						orientation="vertical"
						visibility="{{@data.botCopyPop.setting.visibility}}">
						<!-- 触发价格 -->
						<LinearLayout
							width="match_parent"
							height="wrap_content"
							gravity="center"
							marginTop="8"
							orientation="vertical">
							<LinearLayout
								width="match_parent"
								height="40"
								gravity="center"
								orientation="horizontal">
								<BottomLineTextView 
									height="20" 
									width="wrap_content"
									gravity="left"
									text="{{@i18n/n_contract_grid_create_trigger_price}}"
									textColor="@color/kColorSecondaryText" 
									textFontWeight="400" 
									textSize="14" 
									onClick="@event.botCopyPop.remindClick('trigger')"/>
								<View height="match_parent" width="0" weight="1"/>
								<ImageView height="16" width="28" src="{{@data.botCopyPop.setting.trigger.iconSrc}}" onClick="@event.botCopyPop.triggerClick()"/>
							</LinearLayout>
							<LinearLayout
								width="match_parent"
								height="wrap_content"
								gravity="center"
								orientation="vertical"
								marginBottom="8"
								visibility="{{@data.botCopyPop.setting.trigger.visibility}}">
								<!-- 数量输入 -->
								<LinearLayout
									width="match_parent"
									height="44"
									background="@color/KBaseColorInputBackground"
									cornerRadius="4"
									gravity="center"
									orientation="horizontal">	
									<EditText
										textColorHint="@color/eButtonUnenabledBgColor"
										width="1" 
										height="match_parent" 
										marginLeft="12"
										marginRight="12"
										hint="{{@i18n/n_contract_grid_create_trigger_price}}"
										bindText="@data.botCopyPop.setting.trigger.triggerPrice"
										textFontWeight="400" 
										textSize="14"
										textColor="@color/kColorPrimaryText"
										inputType="numberDecimal"
										textFilter="@dataParser.trade_parser(18,@data.botCopyPop.precision,@eventParams)"
										onTextChange="@event.botCopyPop.onTextChange('triggerPrice',@eventParams)"
										onFocusChange="@event.botCopyPop.onFocusChange('triggerPrice',@eventParams)"
										weight="1"/>
									<LinearLayout 
										width="wrap_content" 
										height="match_parent" 
										gravity="center"
										marginRight="12"
										onClick="@event.botCopyPop.showOptionPop('triggerPrice')">
										<TextView 
											width="wrap_content" 
											height="match_parent" 
											gravity="center"
											text="{{@data.botCopyPop.setting.trigger.triggerPriceTypeDisplay}}"
											textColor="@color/kColorPrimaryText" 
											textFontWeight="400" 
											textSize="14"
											/>
										<ImageView height="12" width="12" src="@drawable/edge_engine_trade_bot_arrow_down" marginLeft="4" />
									</LinearLayout>		
								</LinearLayout>
								<TextView 
									width="match_parent" 
									height="wrap_content" 
									gravity="left"
									text="{{@data.botCopyPop.setting.trigger.errorMsg}}"
									textColor="#E94359" 
									textFontWeight="400" 
									textSize="12"
									marginTop="8"
									maxLines="0"
									visibility="{{@data.botCopyPop.setting.trigger.errorVisibility}}"
									/>
							</LinearLayout>
						</LinearLayout>
						<!-- 止盈止损 -->
						<LinearLayout
							width="match_parent"
							height="wrap_content"
							gravity="center"
							orientation="vertical">
							<LinearLayout
								width="match_parent"
								height="40"
								gravity="center"
								orientation="horizontal">
								<BottomLineTextView 
									height="20" 
									width="wrap_content"
									gravity="left"
									text="{{@data.botCopyPop.setting.tpsl.title}}"
									textColor="@color/kColorSecondaryText" 
									textFontWeight="400" 
									textSize="14" 
									onClick="@event.botCopyPop.remindClick('tpsl')"/>
								<View height="match_parent" width="0" weight="1"/>
								<ImageView height="16" width="28" src="{{@data.botCopyPop.setting.tpsl.iconSrc}}" onClick="@event.botCopyPop.tpslClick()"/>
							</LinearLayout>

							<LinearLayout
								width="match_parent"
								height="wrap_content"
								gravity="center"
								orientation="vertical"
								visibility="{{@data.botCopyPop.setting.tpsl.visibility}}">
								<!-- 止盈 -->
								<LinearLayout
									width="match_parent"
									height="44"
									background="@color/KBaseColorInputBackground"
									cornerRadius="4"
									gravity="center"
									marginBottom="8"
									borderColor="#E94359"
									borderWidth="{{@data.botCopyPop.setting.tpsl.takeProfitPriceBorderWidth}}"
									orientation="horizontal">	
									<EditText
										textColorHint="@color/eButtonUnenabledBgColor"
										width="1" 
										height="match_parent" 
										marginLeft="12"
										marginRight="12"
										hint="{{@data.botCopyPop.setting.tpsl.takeProfitPriceTitle}}"
										bindText="@data.botCopyPop.setting.tpsl.takeProfitPrice"
										textFontWeight="400" 
										textSize="14"
										textColor="@color/kColorPrimaryText"
										inputType="numberDecimal"
										textFilter="@dataParser.trade_parser(18,@data.botCopyPop.precision,@eventParams)"
										onTextChange="@event.botCopyPop.onTextChange('takeProfitPrice',@eventParams)"
										onFocusChange="@event.botCopyPop.onFocusChange('takeProfitPrice',@eventParams)"
										weight="1"/>
									<LinearLayout 
										width="wrap_content" 
										height="match_parent" 
										gravity="center"
										marginRight="12"
										onClick="@event.botCopyPop.showOptionPop('takeProfitPrice')">
										<TextView 
											width="wrap_content" 
											height="match_parent" 
											gravity="center"
											text="{{@data.botCopyPop.setting.tpsl.takeProfitPriceTypeDisplay}}"
											textColor="@color/kColorPrimaryText" 
											textFontWeight="400" 
											textSize="14"
											/>
										<ImageView height="12" width="12" src="@drawable/edge_engine_trade_bot_arrow_down" marginLeft="4" />
									</LinearLayout>	
								</LinearLayout>
								<TextView 
									width="match_parent" 
									height="wrap_content" 
									gravity="left"
									text="{{@data.botCopyPop.setting.tpsl.takeProfitPriceErrorMsg}}"
									textColor="#E94359" 
									textFontWeight="400" 
									textSize="14"
									marginBottom="8"
									maxLines="0"
									visibility="{{@data.botCopyPop.setting.tpsl.takeProfitPriceErrorVisibility}}"
									/>
								<!-- 止损 -->
								<LinearLayout
									width="match_parent"
									height="44"
									background="@color/KBaseColorInputBackground"
									cornerRadius="4"
									gravity="center"
									marginBottom="8"
									borderColor="#E94359"
									borderWidth="{{@data.botCopyPop.setting.tpsl.stopLossPriceBorderWidth}}"
									orientation="horizontal">	
									<EditText
										textColorHint="@color/eButtonUnenabledBgColor"
										width="1" 
										height="match_parent" 
										marginLeft="12"
										marginRight="12"
										hint="{{@data.botCopyPop.setting.tpsl.stopLossPriceTitle}}"
										bindText="@data.botCopyPop.setting.tpsl.stopLossPrice"
										textFontWeight="400" 
										textSize="14"
										textColor="@color/kColorPrimaryText"
										inputType="numberDecimal"
										textFilter="@dataParser.trade_parser(18,@data.botCopyPop.precision,@eventParams)"
										onTextChange="@event.botCopyPop.onTextChange('stopLossPrice',@eventParams)"
										onFocusChange="@event.botCopyPop.onFocusChange('stopLossPrice',@eventParams)"
										weight="1"/>
									<LinearLayout 
										width="wrap_content" 
										height="match_parent" 
										gravity="center"
										marginRight="12"
										onClick="@event.botCopyPop.showOptionPop('stopLossPrice')">
										<TextView 
											width="wrap_content" 
											height="match_parent" 
											gravity="center"
											text="{{@data.botCopyPop.setting.tpsl.stopLossPriceTypeDisplay}}"
											textColor="@color/kColorPrimaryText" 
											textFontWeight="400" 
											textSize="14"
											/>
										<ImageView height="12" width="12" src="@drawable/edge_engine_trade_bot_arrow_down" marginLeft="4" />
									</LinearLayout>	
								</LinearLayout>
								<TextView 
									width="match_parent" 
									height="wrap_content" 
									gravity="left"
									text="{{@data.botCopyPop.setting.tpsl.stopLossPriceErrorMsg}}"
									textColor="#E94359" 
									textFontWeight="400" 
									textSize="14"
									marginBottom="8"
									maxLines="0"
									visibility="{{@data.botCopyPop.setting.tpsl.stopLossPriceErrorVisibility}}"
									/>
							</LinearLayout>
						</LinearLayout>


						<!-- 展示机器人 -->
						<LinearLayout
							width="match_parent"
							height="40"
							gravity="center"
							orientation="horizontal">
							<BottomLineTextView 
								height="20" 
								width="wrap_content"
								gravity="left"
								text="{{@i18n/n_contract_grid_create_show_in_ranking}}"
								textColor="@color/kColorSecondaryText" 
								textFontWeight="400" 
								textSize="14"
								onClick="@event.botCopyPop.remindClick('showBot')" />
							<View height="match_parent" width="0" weight="1"/>
							<ImageView height="16" width="28" src="{{@data.botCopyPop.setting.showBot.iconSrc}}" onClick="@event.botCopyPop.showBotClick()"/>
						</LinearLayout>
					</LinearLayout>

				</LinearLayout>
			</LinearLayout>

		</Scroller>

		<FrameLayout
			width="match_parent"
			height="44"
			marginTop="16"
			marginLeft="16"
			marginRight="16">
			<TextView
				height="match_parent"
				width="match_parent"
				gravity="center"
				cornerRadius="4"
				background="{{@data.botCopyPop.botTrend.bottom.bgColor}}"
				text="{{@data.botCopyPop.botTrend.bottom.titleDisplay}}"
				textColor="{{@data.botCopyPop.botTrend.bottom.titleColor}}" 
				textFontWeight="500" 
				textSize="14"
				onClick="@event.botCopyPop.createClick()"
				visibility="{{@data.botCopyPop.botTrend.bottom.createVisibility}}"/>
			<TextView
				height="match_parent"
				width="match_parent"
				gravity="center"
				cornerRadius="4"
				background="@color/kColorMajorTheme100"
				text="{{@i18n/n_trade_bot_sign_service_disclaimer}}"
				textColor="@color/KBaseTextColor"
				textFontWeight="500"
				textSize="14"
				onClick="@event.botCopyPop.agreementClick()"
				visibility="{{@data.botCopyPop.botTrend.bottom.agreementVisibility}}"/>
		</FrameLayout>
		<View
			width="match_parent"
			height="34" />
	</LinearLayout>
	<View height="0" onPopDismiss="@event.couponPop.popDismiss()" popCancelOnTouchOutside="false"
		popModalPositon="bottom" popShow="{{@data.couponPop.couponPopShow}}" popShowMode="modal"
		popTemplate="couponPop.xml" width="0"/>
</LinearLayout>
