<LinearLayout width="match_parent" height="wrap_content" marginBottom="8" orientation="vertical">

    <!-- 榜单标题 tab view  -->
    <Scroller width="match_parent" height="wrap_content" paddingLeft="0" paddingRight="0" orientation="horizontal" scrollToTag="{{@data.ranking.currentTag}}">
        <LinearLayout width="wrap_content" height="40" marginLeft="8" marginRight="8" gravity="bottom|left" orientation="horizontal">

            <For data="@data.ranking.titleData" itemKey="element" typeKey="element">

                <Cell width="wrap_content" height="wrap_content" marginLeft="8" marginBottom="8" marginRight="8">

                    <LinearLayout width="wrap_content" height="wrap_content" gravity="left" orientation="horizontal">

                        <TextView width="wrap_content" height="wrap_content" gravity="center" text="{{@element.title}}" textSize="{{@element.titleSize}}" textColor="{{@element.titleColor}}" textFontWeight="500" onClick="@event.ranking.tabClick('{{@element.tag}}')" tag="{{@element.tag}}"/>

                        <TextView width="6" height="6" cornerRadius="3" background="@color/KBaseRiskTextColorHigh" visibility="{{@element.pointVisibility|gone}}"/>

                    </LinearLayout>
                </Cell>
            </For>
        </LinearLayout>

    </Scroller>

    <!-- 榜单 内容view page -->
    <SliderView width="match_parent" height="wrap_content" direction="horizontal" autoScroll="false" scrollMode="reverse" onSelected="@event.ranking.resetTitleSelectTab(@eventParams)" currentIndex="@data.ranking.currentIndex" startIndex="10">
        <For data="@data.ranking.listData" itemKey="item" typeKey="listType">

            <Cell width="match_parent" height="wrap_content" type="1">

                <LinearLayout width="match_parent" height="wrap_content" orientation="vertical">

                    <For data="@item.data" itemKey="element" typeKey="cellType">

                        <!-- interactionTitle: 带交互功能的title，用户可选择 OK 缺少弹窗点击事件-->
                        <Cell width="match_parent" height="wrap_content" type="interactionTitle">

                            <LinearLayout width="match_parent" height="24" gravity="centerVertical" orientation="horizontal" marginLeft="16" marginRight="16">

                                <LinearLayout width="match_parent" height="wrap_content" weight="1" popTemplate="rankingpop.xml" popCancelOnTouchOutside="true" popBackgroundDimmed="true" popShowMode="modal" popModalPositon="bottom" popShow="{{@data.rankingpop.popshow}}" onPopDismiss="@event.rankingpop.popDismiss()" onClick="@event.ranking.interactionTitleClick()">

                                    <TextView width="wrap_content" height="wrap_content" text="{{@element.leftTitle.titleName | Name}}" textColor="@color/KBaseColorSecondaryText" textSize="11" textFontWeight="400"/>

                                    <ImageView width="4" height="3" layout_gravity="center" marginLeft="6" src="@drawable/edge_engine_k_trade_arrow_down"/>
                                </LinearLayout>


                                <TextView width="90" height="wrap_content" gravity="right" text="{{@element.middleTitle.titleName | Last Price}}" textColor="@color/KBaseColorSecondaryText" marginRight="16" textSize="11" autoSizeTextType="uniform" textFontWeight="400"/>

                                <TextView width="80" height="wrap_content" gravity="right" text="{{@element.rightTitle.titleName | Change}}" textColor="@color/KBaseColorSecondaryText" textSize="11" textFontWeight="400"/>

                            </LinearLayout>
                        </Cell>

                        <!-- title： 普通title  OK-->
                        <Cell width="match_parent" height="wrap_content" type="title">
                            <LinearLayout width="match_parent" height="24" gravity="centerVertical" orientation="horizontal" background="@color/KBaseColorContentBackground" marginLeft="16" marginRight="16">

                                <TextView width="wrap_content" height="wrap_content" text="{{@element.leftTitle.titleName | Name}}" textColor="@color/KBaseColorSecondaryText" textSize="11" textFontWeight="400" weight="1"/>

                                <TextView width="90" height="wrap_content" gravity="right" text="{{@element.middleTitle.titleName | Last Price}}" textColor="@color/KBaseColorSecondaryText" marginRight="16" textSize="11" autoSizeTextType="uniform" textFontWeight="400"/>

                                <TextView width="80" height="wrap_content" gravity="right" text="{{@element.rightTitle.titleName | Change}}" textColor="@color/KBaseColorSecondaryText" textSize="11" autoSizeTextType="uniform" textFontWeight="400"/>

                            </LinearLayout>
                        </Cell>

                        <!-- favoriteTitle：无自选时自选页面的标题 OK-->
                        <Cell width="match_parent" height="wrap_content" type="favoriteTitle">

                            <TextView width="wrap_content" height="24" text="{{@element.favoriteTitle | Name}}" textColor="@color/KBaseColorSecondaryText" background="@color/KBaseColorContentBackground" textSize="11" marginLeft="16" textFontWeight="400" gravity="center|left"/>
                        </Cell>

                        <!-- space： 空cell 占位 OK 调整height -->
                        <Cell width="match_parent" height="wrap_content" type="space">
                            <View width="match_parent" height="0"/>
                        </Cell>

                        <!-- normal： 热榜 自选现货 涨跌幅榜 用此cell OK-->
                        <Cell width="match_parent" height="wrap_content" type="normal">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" cornerRadius="12" background="@color/baseCoinLockTipBg" placeholder="@drawable/home_coin_placeholder_24"/>

                                <LinearLayout width="wrap_content" height="24" orientation="horizontal" gravity="center">

                                    <TextView height="24" width="wrap_content" maxWidth="68" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="center" marginRight="0" autoSizeTextType="uniform" textColor="@color/kColorPrimaryText"/>

                                    <TextView height="24" width="wrap_content" maxWidth="35" text="{{@element.quoteName}}" autoSizeTextType="uniform" textSize="12" gravity="center" paddingTop="2" textColor="@color/KBaseColorSecondaryText" visibility="{{@element.isQuoteName|gone}}"/>
                                </LinearLayout>

                                <TextView height="wrap_content" width="wrap_content" text="0 fee" textSize="9" textFontWeight="500" marginLeft="2" marginTop="2" paddingBottom="1" paddingTop="1" paddingLeft="2" paddingRight="2" gravity="center" layout_gravity="center" textColor="@color/KBaseColorMajorTheme100" background="@color/KBaseColorMajorTheme006" cornerRadius="3" visibility="{{@element.isZeroFee|gone}}"/>

                                <ImageView width="14" height="14" marginLeft="4" src="{{@element.flagURL}}" visibility="{{@element.flagViewVisibility|gone}}"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="80" text="{{@element.price}}" isSubscript="true" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="16"/>

                                <TextView height="28" width="80" text="{{@element.ratio}}" textSize="14" textFontWeight="500" gravity="center" cornerRadius="3" textColor="@color/KBaseTextColor" autoSizeTextType="uniform" background="{{@element.colorHex|#E94359}}"/>
                            </LinearLayout>
                        </Cell>

                        <!-- deal： 成交额榜cell OK-->
                        <Cell width="match_parent" height="wrap_content" type="deal">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" placeholder="@drawable/home_coin_placeholder_24" cornerRadius="12" background="@color/baseCoinLockTipBg"/>

                                <TextView height="24" width="wrap_content" maxWidth="95" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="center" autoSizeTextType="uniform" textColor="@color/kColorPrimaryText"/>

                                <TextView height="wrap_content" width="wrap_content" text="0 fee" textSize="9" textFontWeight="500" marginLeft="3" paddingBottom="1" paddingTop="1" paddingLeft="2" paddingRight="2" gravity="center" textColor="@color/KBaseColorMajorTheme100" background="@color/KBaseColorMajorTheme006" cornerRadius="3" visibility="{{@element.isZeroFee|gone}}"/>

                                <ImageView width="14" height="14" marginLeft="4" src="{{@item.flagURL}}" visibility="{{@element.flagViewVisibility|gone}}"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="90" text="{{@element.price}}" isSubscript="true" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="16"/>

                                <TextView height="28" width="80" text="{{@element.volume}}" textSize="14" textFontWeight="500" gravity="center" cornerRadius="3" textColor="@color/kColorPriceGreen" autoSizeTextType="uniform" background="#1500A171"/>
                            </LinearLayout>
                        </Cell>
                        <!-- market ： 市值榜cell OK-->
                        <Cell width="match_parent" height="wrap_content" type="market">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" placeholder="@drawable/home_coin_placeholder_24" cornerRadius="12" background="@color/baseCoinLockTipBg"/>

                                <TextView height="24" width="wrap_content" maxWidth="95" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="center" autoSizeTextType="uniform" textColor="@color/kColorPrimaryText"/>

                                <TextView height="wrap_content" width="wrap_content" text="0 fee" textSize="9" textFontWeight="500" marginLeft="3" paddingBottom="1" paddingTop="1" paddingLeft="2" paddingRight="2" gravity="center" textColor="@color/KBaseColorMajorTheme100" background="@color/KBaseColorMajorTheme006" cornerRadius="3" visibility="{{@element.isZeroFee|gone}}"/>

                                <ImageView width="14" height="14" marginLeft="4" src="{{@item.flagURL}}" visibility="{{@element.flagViewVisibility|gone}}"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="90" text="{{@element.price}}" isSubscript="true" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="16"/>

                                <TextView height="28" width="80" text="{{@element.volume}}" textSize="16" textFontWeight="500" gravity="right|center" cornerRadius="3" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform"/>
                            </LinearLayout>
                        </Cell>
                        <!-- new： 已上新币榜 OK-->
                        <Cell width="match_parent" height="wrap_content" type="new">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" placeholder="@drawable/home_coin_placeholder_24" cornerRadius="12" background="@color/baseCoinLockTipBg"/>

                                <TextView height="24" width="wrap_content" maxWidth="66" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="left|center" autoSizeTextType="uniform" textColor="@color/kColorPrimaryText"/>

                                <TextView height="wrap_content" width="wrap_content" text="0 fee" textSize="10" textFontWeight="500" marginLeft="4" paddingBottom="1" paddingTop="1" paddingLeft="2" paddingRight="2" gravity="center" textColor="@color/KBaseColorMajorTheme100" background="@color/KBaseColorMajorTheme006" cornerRadius="3" visibility="{{@element.isZeroFee|gone}}"/>

                                <ImageView width="14" height="14" marginLeft="4" src="{{@element.flagURL}}" visibility="{{@element.flagViewVisibility|gone}}"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="90" text="{{@element.price}}" isSubscript="true" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="16"/>

                                <LinearLayout height="wrap_content" width="80" marginRight="2" gravity="center" orientation="vertical">
                                    <TextView height="wrap_content" width="80" text="{{@element.ratio}}" textSize="16" textFontWeight="500" gravity="right|center" autoSizeTextType="uniform" textColor="{{@element.colorHex|#E94359}}"/>

                                    <TextView height="wrap_content" width="wrap_content" text="{{@element.beginDateStr}}" textSize="10" textFontWeight="400" layout_gravity="right|center" cornerRadius="3" textColor="@color/kColorThreeLevelText"/>
                                </LinearLayout>
                            </LinearLayout>

                        </Cell>

                        <!-- willUp： 待上新币榜 OK-->
                        <Cell width="match_parent" height="wrap_content" type="willUp">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" background="@color/KBaseColorDeepestBackground" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" placeholder="@drawable/home_coin_placeholder_24" cornerRadius="12" background="@color/baseCoinLockTipBg"/>

                                <TextView height="24" width="wrap_content" maxWidth="66" autoSizeTextType="uniform" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="center" textColor="@color/kColorPrimaryText"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="90" text="{{@element.beginDateStr}}" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="6"/>

                                <LinearLayout width="90" height="16" layout_gravity="center" orientation="horizontal">

                                    <View width="1" height="1" weight="1"/>

                                    <TextView width="17" height="14" gravity="center" cornerRadius="2" textSize="10" text="{{@element.day}}" textColor="@color/KBaseColorContentBackground" background="@color/kColorPrimaryText" textFontWeight="500" visibility="{{@element.showDay|gone}}"/>

                                    <TextView width="wrap_content" height="wrap_content" marginLeft="1.5" marginRight="1.5" gravity="center" text=":" textSize="12" textColor="@color/kColorPrimaryText" textFontWeight="700" visibility="{{@element.showDay|gone}}"/>

                                    <TextView width="17" height="14" gravity="center" cornerRadius="2" textSize="10" text="{{@element.hour}}" textColor="@color/KBaseColorContentBackground" background="@color/kColorPrimaryText" textFontWeight="500"/>

                                    <TextView width="wrap_content" height="wrap_content" marginLeft="1.5" marginRight="1.5" gravity="center" text=":" textSize="12" textColor="@color/kColorPrimaryText" textFontWeight="700"/>

                                    <TextView width="17" height="14" gravity="center" text="{{@element.minute}}" textSize="10" cornerRadius="2" textColor="@color/KBaseColorContentBackground" background="@color/kColorPrimaryText" textFontWeight="500"/>

                                    <TextView width="wrap_content" height="wrap_content" gravity="center" marginLeft="1.5" marginRight="1.5" text=":" textSize="12" textColor="@color/kColorPrimaryText" textFontWeight="700"/>

                                    <TextView width="17" height="14" gravity="center" text="{{@element.second}}" textSize="10" cornerRadius="2" textColor="@color/KBaseColorContentBackground" background="@color/kColorPrimaryText" textFontWeight="500"/>

                                </LinearLayout>
                            </LinearLayout>

                        </Cell>


                        <!-- future: 合约cell OK -->
                        <Cell width="match_parent" height="wrap_content" type="future">

                            <LinearLayout width="match_parent" height="52" paddingLeft="16" paddingRight="16" orientation="horizontal" onClick="@event.ranking.rankingClicked('{{@element.symbol}}', '{{@element.analytics}}', '{{@element.originItemJson}}')" gravity="center">

                                <ImageView width="24" height="24" marginRight="12" gravity="right" src="{{@element.iconURL}}" placeholder="@drawable/home_coin_placeholder_24" cornerRadius="12" background="@color/baseCoinLockTipBg"/>

                                <LinearLayout width="wrap_content" height="42" orientation="vertical" gravity="left|center" layout_gravity="center">

                                    <TextView width="wrap_content" height="wrap_content" maxWidth="66" text="{{@element.baseName}}" textSize="16" textFontWeight="500" gravity="center" autoSizeTextType="uniform" textColor="@color/kColorPrimaryText"/>

                                    <TextView height="wrap_content" width="wrap_content" text="{{@element.contractBusinessTypeTag}}" textSize="10" textFontWeight="400" gravity="center" paddingLeft="4" paddingRight="4" paddingTop="2" paddingBottom="2" cornerRadius="2" background="@color/KLogin_v2_bottom_background" textColor="@color/kColorPrimaryText"/>

                                </LinearLayout>

                                <TextView height="wrap_content" width="wrap_content" text="0 fee" textSize="9" textFontWeight="500" marginLeft="4" paddingBottom="1" paddingTop="1" paddingLeft="2" paddingRight="2" gravity="center" textColor="@color/KBaseColorMajorTheme100" background="@color/KBaseColorMajorTheme006" cornerRadius="3" visibility="{{@element.isZeroFee|gone}}"/>

                                <ImageView width="14" height="14" marginLeft="4" src="{{@element.flagURL}}" visibility="{{@element.flagViewVisibility|gone}}"/>

                                <View width="1" height="1" weight="1"/>

                                <TextView height="24" width="90" text="{{@element.price}}" isSubscript="true" textSize="16" textFontWeight="500" gravity="right|center" textColor="@color/kColorPrimaryText" autoSizeTextType="uniform" marginRight="16"/>

                                <TextView height="28" width="80" text="{{@element.ratio}}" textSize="13" textFontWeight="700" gravity="center" cornerRadius="3" textColor="@color/KBaseTextColor" autoSizeTextType="uniform" background="{{@element.colorHex|#E94359}}"/>
                            </LinearLayout>
                        </Cell>

                        <!-- spotFavorite: 无自选时自选页面的数据cell -->
                        <Cell width="match_parent" height="wrap_content" type="spotFavorite">

                            <LinearLayout width="match_parent" height="71" paddingLeft="16" paddingRight="16" paddingBottom="8" background="@color/KBaseColorContentBackground" direction="horizontal">

                                <!-- left card  -->
                                <FrameLayout width="168" height="63" marginRight="7" cornerRadius="6" borderColor="@color/kHomeEarnOtherBorderColor" borderWidth="0.5" visibility="{{@element.leftItemVisibility|gone}}" onClick="@event.ranking.selectedFavoriteSymbol('{{@element.leftItem.symbol}}')">

                                    <LinearLayout width="match_parent" height="wrap_content" marginLeft="12" marginTop="12" gravity="center|left" direction="horizontal">

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.leftItem.baseName}}" textSize="14" textFontWeight="500" gravity="center" textColor="@color/kColorPrimaryText"/>

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.leftItem.quoteName}}" marginLeft="0" textSize="10" gravity="center" textColor="@color/KBaseColorSecondaryText"/>

                                    </LinearLayout>

                                    <LinearLayout width="match_parent" height="wrap_content" marginLeft="12" marginBottom="12" layout_gravity="bottom" direction="horizontal">

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.leftItem.price}}" isSubscript="true" textSize="12" marginRight="6" textFontWeight="400" gravity="center" textColor="@color/kColorPrimaryText"/>

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.leftItem.ratio}}" textSize="12" textFontWeight="400" gravity="center" textColor="{{@element.leftItem.colorHex}}"/>

                                    </LinearLayout>

                                    <ImageView height="12.5" width="12.5" layout_gravity="right" marginTop="24" marginRight="12" src="{{@element.leftItem.favoriteImage}}"/>

                                </FrameLayout>

                                <!-- right -->
                                <FrameLayout width="168" height="63" cornerRadius="6" borderColor="@color/kHomeEarnOtherBorderColor" borderWidth="0.5" visibility="{{@element.rightItemVisibility|gone}}" onClick="@event.ranking.selectedFavoriteSymbol('{{@element.rightItem.symbol}}')">

                                    <LinearLayout width="match_parent" height="wrap_content" marginLeft="12" marginTop="12" gravity="center|left" direction="horizontal">

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.rightItem.baseName}}" textSize="14" textFontWeight="500" gravity="center" textColor="@color/kColorPrimaryText"/>

                                        <TextView height="wrap_content" width="wrap_content" marginLeft="0" text="{{@element.rightItem.quoteName}}" textSize="10" gravity="center" textColor="@color/KBaseColorSecondaryText"/>

                                    </LinearLayout>

                                    <LinearLayout width="match_parent" height="wrap_content" marginLeft="12" marginBottom="12" layout_gravity="bottom" direction="horizontal">

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.rightItem.price}}" isSubscript="true" marginRight="6" textSize="12" textFontWeight="400" gravity="center" textColor="@color/kColorPrimaryText"/>

                                        <TextView height="wrap_content" width="wrap_content" text="{{@element.rightItem.ratio}}" textSize="12" textFontWeight="400" gravity="center" textColor="{{@element.rightItem.colorHex}}"/>

                                    </LinearLayout>

                                    <ImageView height="12.5" width="12.5" layout_gravity="right" marginTop="24" marginRight="12" src="{{@element.rightItem.favoriteImage}}"/>

                                </FrameLayout>

                            </LinearLayout>

                        </Cell>

                        <!-- favoriteAdd: 无自选时自选页面的添加按钮 OK-->
                        <Cell width="match_parent" height="wrap_content" type="favoriteAdd">

                            <LinearLayout width="match_parent" height="wrap_content" background="@color/KBaseColorContentBackground" orientation="vertical">
                                <TextView width="240" height="40" marginLeft="67" marginRight="67" marginTop="16" marginBottom="16" gravity="center" text="{{@i18n/n_home_add_to_favorite}}" textColor="@color/KBaseTextColor" textFontWeight="500" textSize="14" cornerRadius="4" visibility="{{@element.enableButton}}" background="@color/kColorMajorTheme100" onClick="@event.ranking.addFavoritesClick()"/>

                                <TextView width="240" height="40" marginLeft="67" marginRight="67" marginTop="16" marginBottom="16" gravity="center" text="{{@i18n/n_home_add_to_favorite}}" textColor="@color/KBaseTextColor" textFontWeight="500" textSize="14" cornerRadius="4" visibility="{{@element.disableButton}}" background="@color/kColorDividingLine"/>

                                <TextView height="24" width="match_parent" text="{{@i18n/n_market_add_other_symbol}}" textSize="12" textFontWeight="500" gravity="center" textColor="@color/kColorMajorTheme100" onClick="@event.ranking.goToFavorites()"/>
                            </LinearLayout>

                        </Cell>

                        <!-- favoriteEmpty: 有现货自选 无合约自选时的合约页面 或有合约自选 无现货自选时的现货页面cell   OK -->
                        <Cell width="match_parent" height="wrap_content" background="@color/KBaseColorContentBackground" type="favoriteEmpty">

                            <LinearLayout width="match_parent" height="wrap_content" gravity="center" orientation="vertical">

                                <ImageView height="60" width="60" marginTop="20" marginBottom="12" gravity="center" src="@drawable/index_common_empty_favorites_icon"/>

                                <TextView height="24" width="match_parent" text="{{@i18n/n_market_favorite_empty}}" textSize="12" textFontWeight="500" gravity="center" textColor="@color/kColorThreeLevelText"/>

                                <TextView width="125" height="40" marginTop="16" marginBottom="16" gravity="center" text="{{@i18n/n_home_favorite_add}}" textColor="@color/kColorMajorTheme100" textFontWeight="500" textSize="14" cornerRadius="4" background="@color/KBaseColorMajorTheme006" onClick="@event.ranking.goToFavorites('{{@element.subType}}')"/>
                            </LinearLayout>

                        </Cell>

                        <!-- empty: 空数据页面 当页面没数据时显示 OK-->
                        <Cell width="match_parent" height="wrap_content" background="@color/KBaseColorContentBackground" type="empty">

                            <LinearLayout width="match_parent" height="wrap_content" gravity="center" orientation="vertical">

                                <ImageView height="60" width="60" marginTop="20" marginBottom="12" gravity="center" src="@drawable/common_empty_icon"/>

                                <TextView height="24" width="match_parent" text="{{@i18n/global_empty_no_result}}" textSize="12" textFontWeight="500" gravity="center" textColor="@color/kColorThreeLevelText"/>
                            </LinearLayout>

                        </Cell>

                        <!-- more：查看更多 OK-->
                        <Cell width="match_parent" height="wrap_content" type="more">

                            <LinearLayout width="match_parent" height="24" gravity="center" orientation="horizontal" onClick="@event.ranking.moreButtonClicked('{{@element.type}}', '{{@element.subType}}', '{{@element.analytics}}')">
                                <TextView height="wrap_content" width="wrap_content" text="{{@i18n/n_home_view_more}}" textSize="12" textFontWeight="500" gravity="center" textColor="@color/KBaseColorMajorTheme100"/>

                                <ImageView height="10" width="6" gravity="center" marginLeft="7" marginBottom="0.5" src="@drawable/edge_engine_home_blue_right_arrows"/>

                            </LinearLayout>


                        </Cell>

                    </For>

                </LinearLayout>

            </Cell>
        </For>
    </SliderView>

</LinearLayout>
