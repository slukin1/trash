<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="line">
            <stroke android:width="@dimen/dimen_1" android:color="@color/kline_drawing_line_size_selected" android:dashWidth="@dimen/dimen_4" android:dashGap="@dimen/dimen_1"/>
        </shape>
    </item>
    <item>
        <shape android:shape="line">
            <stroke android:width="@dimen/dimen_1" android:color="@color/kline_drawing_line_size_light" android:dashWidth="@dimen/dimen_4" android:dashGap="@dimen/dimen_1"/>
        </shape>
    </item>
</selector>
