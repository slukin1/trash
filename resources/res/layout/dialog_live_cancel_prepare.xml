<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/root_view" android:paddingLeft="25dp" android:paddingRight="25dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/rl_content" android:background="@drawable/bg_live_cancel_prepare" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_centerInParent="true">
        <TextView android:textSize="16sp" android:textColor="@color/color_14181F" android:id="@+id/tv_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="20dp" android:text="是否确认取消预约?"/>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="@dimen/dimen_40" android:layout_marginLeft="19dp" android:layout_marginTop="10dp" android:layout_marginRight="19dp" android:layout_marginBottom="@dimen/dimen_20" android:layout_below="@+id/tv_title">
            <TextView android:textSize="16sp" android:textColor="@color/color_14181F" android:gravity="center" android:id="@+id/tv_cancel" android:background="@drawable/bg_live_prepare_cancel_btn" android:layout_width="0dp" android:layout_height="match_parent" android:text="取消" android:layout_weight="1"/>
            <TextView android:textSize="16sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_confirm" android:background="@drawable/bg_live_hot_talk_into" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginLeft="10dp" android:text="确认" android:layout_weight="1"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
