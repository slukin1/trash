<?xml version="1.0" encoding="utf-8"?>
<com.huobi.view.roundview.RoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/index_earn_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/dimen_8" app:rv_backgroundColor="@color/baseColorContentBackground" app:rv_cornerRadius="@dimen/dimen_8">
    <RelativeLayout android:id="@+id/rl_huo_bi_earn" android:layout_width="match_parent" android:layout_height="40dp">
        <TextView android:textSize="@dimen/global_text_size_16" android:textColor="#000000" android:id="@+id/rv_huo_bi_earn_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:fontFamily="@font/roboto_medium" android:paddingStart="@dimen/dimen_16"/>
