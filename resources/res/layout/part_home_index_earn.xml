        <TextView android:textSize="10sp" android:textColor="#909090" android:gravity="center" android:id="@+id/tv_prime_earn_apy_text" android:layout_width="wrap_content" android:layout_height="15dp" android:layout_marginTop="47dp" android:layout_marginStart="@dimen/dimen_16"/>
        <TextView android:textSize="@dimen/dimen_14" android:textColor="#000000" android:id="@+id/tv_prime_earn_deadline" android:layout_width="wrap_content" android:layout_height="20dp" android:layout_marginTop="26dp" android:fontFamily="@font/roboto_medium"/>
