        <TextView android:textSize="16sp" android:textColor="@color/baseColorMajorTheme100" android:gravity="center" android:id="@+id/tv_prime_earn_apy" android:layout_width="wrap_content" android:layout_height="21dp" android:layout_marginTop="24dp" android:maxLines="1" android:includeFontPadding="false" android:fontFamily="@font/roboto_medium" android:layout_marginStart="@dimen/dimen_16" android:autoSizeTextType="1"/>
