        <TextView android:textSize="16sp" android:textColor="@color/color_live_square_two_base" android:gravity="center" android:id="@+id/tv_member_kick" android:layout_width="match_parent" android:layout_height="50dp" android:text="@string/n_im_forbin_kick"/>
        <View android:id="@+id/v_member_kick" android:background="@color/base_dialog_devide_line_color" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <TextView android:textSize="16sp" android:textColor="@color/color_live_square_two_base" android:gravity="center" android:id="@+id/tv_member_set_manager" android:layout_width="match_parent" android:layout_height="50dp" android:text="@string/n_im_set_manager"/>
        <View android:background="@color/dialog_line_color" android:layout_width="match_parent" android:layout_height="5dp"/>
        <TextView android:textSize="16sp" android:textColor="@color/baseColorSecondaryText" android:gravity="center" android:id="@+id/tv_cancel" android:layout_width="match_parent" android:layout_height="50dp" android:text="@string/n_cancel"/>
