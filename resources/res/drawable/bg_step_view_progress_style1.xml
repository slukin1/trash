<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dimen_0"/>
            <gradient android:startColor="@color/baseColorPrimarySeparator" android:endColor="@color/baseColorPrimarySeparator" android:angle="0" android:centerY="1" android:centerColor="@color/baseColorPrimarySeparator"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dimen_0"/>
                <gradient android:startColor="@color/baseColorMajorTheme100" android:endColor="@color/baseColorMajorTheme100" android:angle="0" android:centerY="1" android:centerColor="@color/baseColorMajorTheme100"/>
            </shape>
        </clip>
    </item>
</layer-list>
