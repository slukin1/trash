<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dimen_2"/>
            <solid android:color="@color/balance_head_fuc_btn_stoke_color"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="@dimen/dimen_2"/>
                <solid android:color="@color/baseTextColor"/>
            </shape>
        </scale>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape>
            <corners android:radius="@dimen/dimen_2"/>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
</layer-list>
