<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/zui_color_white_100"/>
        </shape>
    </item>
    <item android:left="-2dp" android:right="-2dp" android:bottom="-2dp">
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="@color/zui_color_black_12"/>
        </shape>
    </item>
</layer-list>
