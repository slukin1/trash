<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true">
        <shape>
            <stroke android:width="@dimen/dimen_1" android:color="#0173e5"/>
            <solid android:color="#313132"/>
            <corners android:radius="@dimen/dimen_4"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <stroke android:width="@dimen/dimen_1" android:color="#0173e5"/>
            <solid android:color="#313132"/>
            <corners android:radius="@dimen/dimen_4"/>
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <solid android:color="#313132"/>
            <corners android:radius="@dimen/dimen_4"/>
        </shape>
    </item>
</selector>
