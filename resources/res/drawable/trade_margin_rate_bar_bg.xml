<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <clip>
            <shape>
                <gradient android:startColor="@color/baseColorContentBackground" android:endColor="@color/baseColorContentBackground" android:angle="360"/>
                <corners android:topRightRadius="@dimen/dimen_1" android:bottomLeftRadius="@dimen/dimen_5" android:bottomRightRadius="@dimen/dimen_1"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/baseColorThreeLevelText" android:endColor="@color/baseColorThreeLevelText" android:angle="360"/>
                <corners android:topRightRadius="@dimen/dimen_1" android:bottomLeftRadius="@dimen/dimen_10" android:bottomRightRadius="@dimen/dimen_1"/>
            </shape>
        </clip>
    </item>
</layer-list>
