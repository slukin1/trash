<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/dimen_4"/>
            <solid android:color="@color/login_v2_login_button_enable_bg"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/dimen_4"/>
            <solid android:color="@color/login_v2_login_button_enable_bg"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dimen_4"/>
            <solid android:color="@color/login_v2_login_button_enable_bg"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/dimen_4"/>
            <solid android:color="@color/baseColorDisableButton"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/dimen_4"/>
            <solid android:color="@color/login_v2_login_button_enable_bg"/>
        </shape>
    </item>
</selector>
