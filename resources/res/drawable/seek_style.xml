<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background" android:top="1.75dp" android:bottom="1.75dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/kline_current_price_tag_shadow_color"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress" android:top="1.75dp" android:bottom="1.75dp">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="@color/kline_setting_text_color"/>
                <corners android:radius="100dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
