<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/baseColorMiniButton"/>
            <stroke android:width="@dimen/dimen_1" android:color="@color/baseColorDefaultPlaceholder"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="@dimen/dimen_1" android:color="@color/baseColorDefaultPlaceholder"/>
            <corners android:radius="0dp"/>
        </shape>
    </item>
</selector>
